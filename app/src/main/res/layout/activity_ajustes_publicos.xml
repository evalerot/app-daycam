<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="20dp"
    android:orientation="vertical"
    tools:context=".Presentacion.AjustesPublicos">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:orientation="vertical"
        android:gravity="center"
        tools:ignore="UselessParent">

        <Button
            android:id="@+id/registrar1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Registrar Cel 1"
            android:fontFamily="serif"
            android:textStyle="bold"/>

        <Button
            android:id="@+id/registrar2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Registrar Cel 2"
            android:fontFamily="serif"
            android:textStyle="bold"/>

        <Button
            android:id="@+id/registrar3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Registrar Cel 3"
            android:fontFamily="serif"
            android:textStyle="bold"/>

        <Button
            android:id="@+id/registrar4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Registrar Cel 4"
            android:fontFamily="serif"
            android:textStyle="bold"/>

        <Button
            android:id="@+id/registrar5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Registrar Cel 5"
            android:fontFamily="serif"
            android:textStyle="bold"/>
    </LinearLayout>
</LinearLayout>